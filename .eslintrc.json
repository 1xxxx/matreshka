{
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-var": "error",
        "no-else-return": 0,
        "prefer-rest-params": 0,
        "new-cap": ["error", {"capIsNewExceptions": ["Class"]}],
        "comma-dangle": 0
    },
    "env": {
        "jasmine": true
    },
    "globals": {
        "nofn": true
    }
}
