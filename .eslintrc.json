{
	"extends": "airbnb",
	"rules": {
		"indent": ["error", "tab"],
		"no-var": "error",
		"no-else-return": 0,
		"prefer-rest-params": 0,
		"new-cap": ["error", {"capIsNewExceptions": ["Class"]}],
		"comma-dangle": 0,
		"spaced-comment": 0,
		"no-cond-assign": 0
	},
	"env": {
		"jasmine": true
	},
	"globals": {
		"nofn": true
	}
}
